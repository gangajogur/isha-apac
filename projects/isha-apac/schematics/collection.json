{
  "$schema": "../../../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "ng-add": {
      "description": "Add my library to the project.",
      "factory": "./ng-add/index#ngAdd",
      "schema": "./ng-add/schema.json"
    },
    "setup-ide": {
      "description": "Add vscode IDE setup files in .vscode folder",
      "factory": "./ide-setup/index#setupIde",
      "schema": "./ide-setup/schema.json"
    },
    "setup-eslint": {
      "description": "Add eslint to the project",
      "factory": "./setup-eslint/index#setupEsLint",
      "schema": "./setup-eslint/schema.json"
    },
    "eslint-schematic-private": {
      "description": "Add eslint schematics during IDE setup schematics",
      "factory": "./setup-eslint/index#eslintSchematicPrivate",
      "schema": "./setup-eslint/schema.json"
    },
    "setup-project": {
      "description": "Add project setup to the project",
      "factory": "./setup-proj/index#setupProject",
      "schema": "./setup-proj/schema.json"
    },
    "angular-material-schematic-private": {
      "description": "Add eslint schematics during IDE setup schematics",
      "factory": "./setup-proj/index#angularMaterialSchematicPrivate",
      "schema": "./setup-proj/schema.json"
    },
    "setup-i18n": {
      "description": "Add internationalisation (i18n) support",
      "factory": "./setup-i18n/index#setupI18n",
      "schema": "./setup-i18n/schema.json"
    },
    "shared-module": {
      "description": "Add shared module",
      "factory": "./shared-module/index#setupSharedModule",
      "schema": "./shared-module/schema.json"
    }
  }
}
