{
  "name": "ngx-cli-library-test",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-ngx-cli-library": "cd ../ngx-cli-library && npm run compile-test",
    "install-ngx-cli-library": "npm uninstall @isha-apac/ngx-cli-library && npm run build-ngx-cli-library && npm i ../../dist/ngx-cli-library --install-links",
    "install-ngx-cli-library-from-npm": "npm i @isha-apac/ngx-cli-library@latest",
    "ng-add-ngx-cli-library": "npm run install-ngx-cli-library && ng g @isha-apac/ngx-cli-library:ng-add"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@isha-apac/ngx-cli-library": "file:../../dist/ngx-cli-library",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.6",
    "@angular/cli": "~14.2.6",
    "@angular/compiler-cli": "^14.2.0",
    "@types/jasmine": "~4.0.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.7.2"
  }
}
